#include<bits/stdc++.h>
#define F(i, a, b) for(int i = a; i < b; ++i)
using namespace std;
int a[100005];
int main()
{
    int t;
    cin >> t;
    while(t--)
    {
        int n, k;
        cin >> n >> k;
        F(i, 0, n) cin >> a[i];
        sort(a, a + n);
        cout << a[k - 1];
        cout << endl;
    }
    return 0;
}
