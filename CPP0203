#include<bits/stdc++.h>
#define ll long long
#define FOR(i, a, b) for(int i = a; i <= b; ++i)
using namespace std;
int a[100005];
int main()
{
    int t;
    cin >> t;
    while(t--)
    {
        int n;
        cin >> n;
        int dd[100005] = {};
        FOR(i, 1, n)
        {
            cin >> a[i];
            if(a[i] > 0) dd[a[i]]++;
        }
        FOR(i, 1, 1000000){
        if(!dd[i])
        {
            cout << i << endl;
            break;
        }
    }
}
    return 0;
}
