#include<bits/stdc++.h>
using namespace std;
long long gcd(long long a, long long b){
	if(b == 0) return a;
	return gcd(b, a % b);
}
long long lcm(long long a, long long b){
	return (a / gcd(a, b)) * b;
}
long long kq(long long n){
	long long a = 1;
	for(int i = 1 ;i <= n;i++){
	a = lcm(a, i);
	}
	return a;	
}
int main(){	
    int t;
    cin >> t;
    while(t--){
   long long n;
    cin >> n;
    cout << kq(n) << endl;
	}

}
	
	
	
	

