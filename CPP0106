#include <bits/stdc++.h>
using namespace std;
using ll = long long;
bool tn(string s){
     string tmp = s;
     reverse(tmp.begin(), tmp.end());
     return tmp == s;
}
int main(){
     int t;
     cin >> t;
     cin.ignore();
     string s;
     while(t--){
     cin >> s;
     if(tn(s)) cout << "YES" << endl;
     else cout << "NO" << endl;
     }
}
