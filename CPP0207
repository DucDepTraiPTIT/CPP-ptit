#include<bits/stdc++.h>
#define ll long long
#define F(i, a, b) for(int i = a; i < b; ++i)
using namespace std;
int main()
{
    int t;
    cin >> t;
    while(t--)
    {
        int n, d;
        cin >> n >> d;
        int *a = new int [n];
        F(i, 0, n) cin >> *(a + i);
        F(i, d, n) cout << *(a + i) << " ";
        F(i, 0, d) cout << *(a + i) << " ";
        cout << endl;
    }
    return 0;
}
