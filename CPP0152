#include<bits/stdc++.h>
using namespace std;
void solve(int a, int m){
    int ans = -1;
    for(int i = 0 ; i < m;++i){
        if(a * i % m == 1){
            ans = i;
            break;
        }
    }
    cout << ans;
}
int main(){
  int t;
  cin >> t;
  while(t--){
    int a, m;
    cin >> a >> m;
    solve(a, m);
    cout << endl;
}
}
