#include<bits/stdc++.h>
using namespace std;
int main(){
    char a[15]= {'A','B','B','A','D','C','C','A','B','D','C','C','A','B','D'};
    char b[15]= {'A','C','C','A','B','C','D','D','B','B','C','D','D','B','B'};
    int t;
    cin >> t;
    while(t--){
        int x;
        cin >> x;
        double sum = 0;
    if(x == 101){
        for(int i = 0 ; i < 15;i++){
            char c;
            cin >> c;
            if(c == a[i]) sum += float(2) / 3;
        }
        cout << setprecision(2) << fixed << sum << endl;
    }
    else{
        for(int i = 0 ; i < 15;i++){
            char c;
            cin >> c;
            if(c == b[i]) sum += float(2) / 3;
        }
        cout << setprecision(2) << fixed << sum << endl;
    }
}
}
