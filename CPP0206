#include<bits/stdc++.h>
#define ll long long
#define FOR(i, a, b) for(int i = a; i <= b; ++i)
using namespace std;
int a[100005];
int main()
{
    int t;
    cin >> t;
    while(t--)
    {
        int n;
        cin >> n;
        FOR(i, 1, n)
        {
            cin >> a[i];
        }
        sort(a + 1, a + 1 + n);
        cout << a[n] << endl;
    }
    return 0;
}
